    <groupId>com.iwaneez.jsf</groupId>
    <artifactId>simple_jsf_app</artifactId>
    <packaging>war</packaging>
    <version>1.0-SNAPSHOT</version>
    <name>simple_jsf_app Maven Webapp</name>
